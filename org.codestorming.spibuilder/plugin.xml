<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="spiBuilder"
         name="SPI Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            callOnEmptyDelta="true"
            hasNature="false">
         <run
               class="org.codestorming.spibuilder.IncrementalSPIBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="org.codestorming.spibuilder.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment/spibuilderFragment.e4xmi">
      </fragment>
   </extension>

</plugin>
